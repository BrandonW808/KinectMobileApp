{
  "expo": {
    "name": "Kinect",
    "slug": "Kinect",
    "version": "1.0.6",
    "orientation": "portrait",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#BC313A"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.kinect.kinectapp",
      "buildNumber": "1.0.6",
      "supportsTablet": true,
      "icon": "./assets/faviconapple.png",
    },
    "android": {
      "package": "com.kinect.kinect",
      "versionCode": 12,
      "permissions": [],
      "icon": "./assets/favicon.png",
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
